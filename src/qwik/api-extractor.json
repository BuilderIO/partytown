{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "bundledPackages": ["react"],
  "compiler": {
    "tsconfigFilePath": "<projectFolder>/tsconfig.json"
  },
  "mainEntryPointFilePath": "<projectFolder>/tsc/src/react/index.d.ts",
  "apiReport": {
    "enabled": true,
    "reportFileName": "api.md",
    "reportFolder": "<projectFolder>/src/react/",
    "reportTempFolder": "<projectFolder>/.cache/api-extractor/react/"
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "<projectFolder>/react/index.d.ts"
  },
  "docModel": {
    "enabled": false
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "none"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "error"
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "error"
      }
    }
  }
}
